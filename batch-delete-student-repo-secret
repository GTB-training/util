for repo in $(gtb student repo list li.xiang)
do
    for secret in $(gtb student secret list $repo li.xiang 2>/dev/null | tr '\t' ' ' | cut -d' ' -f1)
    do
        gtb student secret remove $repo $secret li.xiang
    done
done

for repo in $(gtb student repo list li.xiang)
do
    gtb student secret list $repo li.xiang
done

